{
  "Comment": "A simple example",
  "StartAt": "Processa Payload",
  "States": {
    "Processa Payload": {
      "Type": "Map",
      "InputPath": "$",
      "ItemsPath": "$.Payload",
      "Iterator": {
        "StartAt": "Processa Modulo",
        "States": {
          "Processa Modulo": {
            "Type": "Map",
            "InputPath": "$",
            "ItemsPath": "$.execucao",
            "Iterator": {
              "StartAt": "Processa Passo",
              "States": {
                "Processa Passo": {
                  "Type": "Pass",
                  "Parameters": {
                    "FullName.$": "States.Format('Name {}', $.name)",
                    "Table.$": "States.Format('Query {}', $.tabelas[0].tabela.GlueJobParameters.query)"
                  },
                  "ResultPath": "$.Message",
                  "End": true
                }
              }
            },
            "End": true
          }
        }
      },
      "End": true
    }
  }
}